html
    head
        title= title
    body
        h1= title
        form(action="/upload", method="post", enctype="multipart/form-data")
            div 
                label(for="images")= "Choose Pic"
                input(type="file", name="images", id="images")
            div
                button="send"
        h2= "images"
        each image in images
            img(src="./uploads/"+image, style="height: 60px")

